{
  "plugins": ["jest"],
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "es6": true,
    "commonjs": true,
    "jest": true
  },
  "rules": {
    "semi": 1,
    "eqeqeq": 1,
    "no-console": 0,
    "object-curly-spacing": [1, "always"],
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used" }],
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
  }
}
