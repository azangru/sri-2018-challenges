{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true
  },
  "rules": {
    "semi": 1,
    "eqeqeq": 1,
    "no-console": 0,
    "object-curly-spacing": [1, "always"],
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used" }],
  }
}
